@using System.Web.UI.WebControls
@{
	ViewBag.Title = "GiveCamp Houston";
}

<div class="main-container">
	<div class="container">
		<img class="logo" src="@Url.Content("~/Images/give-camp-houston-logo.png")"/>
		<h1>@ViewBag.Title</h1>
		<h2><span>April 4 - 6, 2014</span>&bull;<span>Houston, Texas</span></h2>
		
		<div class="register-button">
			<a href="https://www.eventbrite.com/e/2014-houston-givecamp-registration-10032769283?ref=ebtnebregn" target="_blank"><img src="http://www.eventbrite.com/custombutton?eid=10032769283" alt="Eventbrite - 2014 Houston GiveCamp" /></a>
		</div>
		
		<div class="row">
			<div class="descriptions col-xs-10 col-xs-offset-1 col-sm-12 col-sm-offset-0 col-md-10 col-md-offset-1 col-lg-8 col-lg-offset-2">
				<div class="row">
					<div class="col-xs-12 col-sm-6">
						<p><strong>GiveCamp</strong> is a three day event where technology professionals from designers, developers and database administrators to marketers and web strategists donate their time to provide solutions for non-profit organizations.</p>
					</div>
			
					<div class="col-xs-12 col-sm-6">
						<p><strong>GiveCamp Houston</strong> is being hosted this year by <em>David Weekley Homes</em>. We are hoping for a large turnout and are looking forward to seeing you all there. Sign up soon and come join us!</p>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>